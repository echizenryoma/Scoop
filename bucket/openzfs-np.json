{
    "version": "2.4.1rc2",
    "description": "The open source port of OpenZFS on Windows",
    "homepage": "https://openzfsonwindows.org",
    "license": "Freeware",
    "url": "https://github.com/openzfsonwindows/openzfs/releases/download/zfswin-2.4.1rc2/OpenZFSOnWindows-debug-2.4.1rc2.exe#/setup.exe",
    "hash": "82180dc77f1a16548f989780926fc876a7456b89fe492add9af9f3261467f762",
    "installer": {
        "args": [
            "/VERYSILENT",
            "/NOCANCEL",
            "/NORESTART",
            "/CLOSEAPPLICATIONS"
        ]
    },
    "checkver": {
        "script": [
            "$resp = Invoke-WebRequest -Uri 'https://api.github.com/repos/openzfsonwindows/openzfs/releases'",
            "$json = $resp.Content | ConvertFrom-Json",
            "$json[0].tag_name -replace 'zfswin-',''"
        ],
        "regex": "(?<version>.+)"
    },
    "autoupdate": {
        "url": "https://github.com/openzfsonwindows/openzfs/releases/download/zfswin-$version/OpenZFSOnWindows-debug-$version.exe#/setup.exe"
    }
}
